; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={
}

[application]

config/name="WOiRDo"
run/main_scene="res://01_scenes/Main.tscn"
boot_splash/image="res://03_assets/Logo/logoV2_back.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )

[autoload]

LettersAndNumbers="*res://02_scripts/Autoload/LettersAndNumbers.gd"
Signals="*res://02_scripts/Autoload/Signals.gd"
Content="*res://02_scripts/Autoload/Content.gd"

[display]

window/size/width=576
window/size/height=1024
window/size/always_on_top=true
window/size/test_width=288
window/size/test_height=512
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[global]

w=false
alwa=false
on_top=false

[memory]

limits/message_queue/max_size_kb=16384

[physics]

common/enable_pause_aware_picking=true

[rendering]

limits/buffers/blend_shape_max_buffer_size_kb=8192
limits/buffers/immediate_buffer_size_kb=4096
environment/default_environment="res://default_env.tres"
